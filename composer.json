{
    "name": "techfix/support",
    "description": "TechFix Laravel Support Package",
    "keywords": [
        "laravel",
        "techfix",
        "support"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "TechFix",
            "email": "support@techfix.org"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "~5.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "TechFix\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TechFix\\Support\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "TechFix\\Support\\SupportProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
